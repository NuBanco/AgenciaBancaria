<!DOCTYPE hibernate-mapping PUBLIC

   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

    package="br.agencia">

    <class name="Usuario">

        <id name="codigo" column="idUsuario" type="integer"/>

        <property name="login" />

        <property name="senha" />

        <property name="senhaOperacao" />

        <property name="tipoUsuario" />

    </class>

</hibernate-mapping>
